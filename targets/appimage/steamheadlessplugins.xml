<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
    <id>steamheadlessplugins</id>
    <metadata_license>GPLv3</metadata_license>
    <project_license>GPLv3</project_license>
    <name>SteamHeadlessPlugins</name>
    <summary>SteamHeadlessPlugins</summary>
    <description>
        <p>  Python {{ python-fullversion }} with the Steam Headless Plugins
             bundled in an AppImage.
        </p>
    </description>
    <launchable type="desktop-id">steamheadlessplugins.desktop</launchable>
    <url type="homepage">https://github.com/Steam-Headless/</url>
    <provides>
        <binary>python{{ python-version }}</binary>
    </provides>
</component>
